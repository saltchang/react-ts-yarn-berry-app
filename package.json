{
  "name": "react-ts-yarn-berry-app",
  "version": "0.1.0",
  "packageManager": "yarn@3.1.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
    "start": "react-scripts --openssl-legacy-provider start",
    "build": "react-scripts --openssl-legacy-provider build",
    "serve": "yarn dlx serve -s build",
    "test": "react-scripts test --verbose --coverage --watchAll=false",
    "test-watch": "react-scripts test --verbose --coverage --watchAll",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{js,ts,jsx,tsx}'",
    "lint-fix": "eslint --fix 'src/**/*.{js,ts,jsx,tsx}'",
    "lint-all": "eslint '**/*.{js,ts,jsx,tsx}'",
    "format": "prettier --write '**/*.{js,ts,jsx,tsx,json,css,scss,html,md,yml}'"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^2.1.2",
    "workbox-background-sync": "^6.4.1",
    "workbox-broadcast-update": "^6.4.1",
    "workbox-cacheable-response": "^6.4.1",
    "workbox-core": "^6.4.1",
    "workbox-expiration": "^6.4.1",
    "workbox-google-analytics": "^6.4.1",
    "workbox-navigation-preload": "^6.4.1",
    "workbox-precaching": "^6.4.1",
    "workbox-range-requests": "^6.4.1",
    "workbox-routing": "^6.4.1",
    "workbox-strategies": "^6.4.1",
    "workbox-streams": "^6.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/eslint-plugin": "^7.14.5",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/babel__core": "^7.1.16",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.10",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.0",
    "typescript": "^4.5.2"
  }
}
